{
  "$schema": "https://schema.tauri.app/config/2.0.0",
  "productName": "sudoku-magic",
  "version": "0.2.2",
  "identifier": "com.silentdusk.sudoku-magic",
  "build": {
    "beforeDevCommand": "yarn dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "yarn build",
    "frontendDist": "../build"
  },
  "app": {
    "windows": [
      {
        "title": "Sudoku Magic",
        "width": 400,
        "height": 600,
        "minHeight": 600,
        "minWidth": 400
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}